<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".BlankFragment" android:id="@+id/frameLayout">

    <!-- TODO: Update blank fragment layout -->
    <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/letrasfra" app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="10dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="150dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="20dp"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginBottom="650dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <WebView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="100dp"
            app:layout_constraintTop_toBottomOf="@+id/letrasfra" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="5dp" android:id="@+id/web"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonbus" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/letrasfra" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginBottom="650dp"
            app:layout_constraintBottom_toBottomOf="@+id/web"/>
    <ProgressBar
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/barra"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/web"
            android:layout_marginTop="2dp" app:layout_constraintTop_toBottomOf="@+id/letrasfra"
            android:layout_marginBottom="2dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>